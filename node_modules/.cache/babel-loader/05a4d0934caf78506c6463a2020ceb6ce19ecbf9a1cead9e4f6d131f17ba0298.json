{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'MainApp',\n  data() {\n    return {\n      tweets: [],\n      newTweetContent: ''\n    };\n  },\n  mounted() {\n    this.fetchTweets();\n  },\n  methods: {\n    fetchTweets() {\n      axios.get('http://localhost:3000/tweets').then(response => {\n        this.tweets = response.data;\n      }).catch(error => {\n        console.error(\"There was an error fetching the tweets!\", error);\n      });\n    },\n    addTweet() {\n      const newTweet = {\n        content: this.newTweetContent,\n        user: {\n          avatar: \"https://pbs.twimg.com/profile_images/1121328878142853120/e-rpjoJi_bigger.png\",\n          name: \"User Name\",\n          handle: \"@username\"\n        },\n        created_at: new Date().toLocaleString()\n      };\n      axios.post('http://localhost:3000/tweets', newTweet).then(response => {\n        console.log(response); // Example usage of response\n        this.tweets.push(response.data); // Assuming your API returns the new tweet object with an ID\n        this.newTweetContent = ''; // Clear input field\n      }).catch(error => {\n        console.error(\"There was an error adding the tweet!\", error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","tweets","newTweetContent","mounted","fetchTweets","methods","get","then","response","catch","error","console","addTweet","newTweet","content","user","avatar","handle","created_at","Date","toLocaleString","post","log","push"],"sources":["C:\\Users\\Master\\OneDrive\\Documents\\VueJs\\projet-vuejs3-basic\\src\\components\\Main.vue"],"sourcesContent":["<template>\r\n  <div class=\"border border-gray-600 h-auto border-t-0 w-full\">\r\n    <!-- Header -->\r\n    <div class=\"flex\">\r\n      <div class=\"flex-1 m-2\">\r\n        <h2 class=\"px-4 py-2 text-xl font-semibold text-white\">Home</h2>\r\n      </div>\r\n      <div class=\"flex-1 px-4 py-2 m-2\">\r\n        <!-- Placeholder for other actions or links -->\r\n      </div>\r\n    </div>\r\n\r\n    <hr class=\"border-gray-600\">\r\n\r\n    <!-- Create Tweet Section -->\r\n    <div class=\"flex\">\r\n      <div class=\"m-2 w-10 py-1\">\r\n        <img class=\"inline-block h-10 w-10 rounded-full\" src=\"https://pbs.twimg.com/profile_images/1121328878142853120/e-rpjoJi_bigger.png\" alt=\"User Avatar\" />\r\n      </div>\r\n      <div class=\"flex-1 px-2 pt-2 mt-2\">\r\n        <textarea v-model=\"newTweetContent\" class=\"bg-black text-gray-400 font-medium text-lg w-full\" rows=\"2\" placeholder=\"What's happening?\"></textarea>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Icons Section -->\r\n    <div class=\"flex\">\r\n      <!-- Icons here (optional) -->\r\n\r\n      <div class=\"flex-1\">\r\n        <button @click=\"addTweet\" class=\"bg-blue-400 mt-5 hover:bg-blue-600 text-white font-bold py-2 px-8 rounded-full mr-8 float-right\">\r\n          Tweet\r\n        </button>\r\n      </div>\r\n    </div>\r\n\r\n    <hr class=\"border-gray-600\">\r\n\r\n    <!-- Display Tweets -->\r\n    <div v-for=\"tweet in tweets\" :key=\"tweet.id\" class=\"flex flex-shrink-0 w-full p-4\">\r\n      <div class=\"flex-shrink-0 m-2\">\r\n        <a href=\"#\" class=\"flex title-font font-medium items-center\">\r\n          <img class=\"h-11 w-11 rounded-full\" :src=\"tweet.user.avatar\" alt=\"User Avatar\" />\r\n        </a>\r\n      </div>\r\n      <div class=\"flex-1 bg-transparent rounded-lg px-4 pt-2 mt-2\">\r\n        <div>\r\n          <h2 class=\"text-base leading-6 font-medium text-gray-400\">\r\n            {{ tweet.user.name }} <span class=\"text-sm leading-5 font-medium text-gray-500 group-hover:text-gray-300 transition ease-in-out duration-150\"> {{ tweet.user.handle }} Â· </span>\r\n            <span class=\"text-sm leading-5 font-medium text-gray-500 group-hover:text-gray-300 transition ease-in-out duration-150\">\r\n              {{ tweet.created_at }}\r\n            </span>\r\n          </h2>\r\n        </div>\r\n        <div>\r\n          <p class=\"text-white block text-lg leading-snug mt-3\">\r\n            {{ tweet.content }}\r\n          </p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'MainApp',\r\n  data() {\r\n    return {\r\n      tweets: [],\r\n      newTweetContent: '',\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchTweets();\r\n  },\r\n  methods: {\r\n    fetchTweets() {\r\n      axios.get('http://localhost:3000/tweets')\r\n        .then(response => {\r\n          this.tweets = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.error(\"There was an error fetching the tweets!\", error);\r\n        });\r\n    },\r\n    addTweet() {\r\n      const newTweet = {\r\n        content: this.newTweetContent,\r\n        user: {\r\n          avatar: \"https://pbs.twimg.com/profile_images/1121328878142853120/e-rpjoJi_bigger.png\",\r\n          name: \"User Name\",\r\n          handle: \"@username\"\r\n        },\r\n        created_at: new Date().toLocaleString(),\r\n      };\r\n\r\n      axios.post('http://localhost:3000/tweets', newTweet)\r\n        .then(response => {\r\n          console.log(response); // Example usage of response\r\n          this.tweets.push(response.data); // Assuming your API returns the new tweet object with an ID\r\n          this.newTweetContent = ''; // Clear input field\r\n        })\r\n        .catch(error => {\r\n          console.error(\"There was an error adding the tweet!\", error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Scoped styles if any */\r\n</style>\r\n"],"mappings":";AAgEA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC;EACpB,CAAC;EACDC,OAAO,EAAE;IACPD,WAAW