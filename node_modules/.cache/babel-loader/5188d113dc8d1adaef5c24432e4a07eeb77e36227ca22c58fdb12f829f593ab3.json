ript>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'MainApp',\r\n  data() {\r\n    return {\r\n      tweets: [],\r\n      newTweetContent: ''\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchTweets();\r\n  },\r\n  methods: {\r\n    fetchTweets() {\r\n      axios.get('http://localhost:3000/tweets')\r\n        .then(response => {\r\n          this.tweets = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.error(\"There was an error fetching the tweets!\", error);\r\n        });\r\n    },\r\n    addTweet() {\r\n      const newTweet = {\r\n        content: this.newTweetContent,\r\n        user: {\r\n          avatar: \"https://pbs.twimg.com/profile_images/1121328878142853120/e-rpjoJi_bigger.png\",\r\n          name: \"User Name\",\r\n          handle: \"@username\"\r\n        },\r\n        created_at: new Date().toLocaleString(),\r\n        retweets: 0,\r\n        likes: 0,\r\n        comments: 0\r\n      };\r\n\r\n      axios.post('http://localhost:3000/tweets', newTweet)\r\n        .then(response => {\r\n          this.tweets.push(response.data); // Assuming your API returns the new tweet object with an ID\r\n          this.newTweetContent = ''; // Clear input field\r\n        })\r\n        .catch(error => {\r\n          console.error(\"There was an error adding the tweet!\", error);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Add any additional styles you need here */\r\n</style>\r\n"],"mappings":";AAuFA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC;EACpB,CAAC;EACDC,OAAO,EAAE;IACPD,WAAWA,CAAA,EAAG;MACZN,KAAK,CAACQ,GAAG,CAAC,8BAA8B,EACrCC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACP,MAAK,GAAIO,QAAQ,CAACR,IAAI;MAC7B,CAAC,EACAS,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,yCAAyC,EAAEA,KAAK,CAAC;MACjE,CAAC,CAAC;IACN,CAAC;IACDE,QAAQA,CAAA,EAAG;