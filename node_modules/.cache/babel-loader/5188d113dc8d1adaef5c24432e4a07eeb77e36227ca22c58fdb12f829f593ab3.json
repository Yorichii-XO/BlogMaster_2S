{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'MainApp',\n  data() {\n    return {\n      tweets: [],\n      newTweetContent: ''\n    };\n  },\n  mounted() {\n    this.fetchTweets();\n  },\n  methods: {\n    fetchTweets() {\n      axios.get('http://localhost:3000/tweets').then(response => {\n        this.tweets = response.data;\n      }).catch(error => {\n        console.error(\"There was an error fetching the tweets!\", error);\n      });\n    },\n    addTweet() {\n      const newTweet = {\n        content: this.newTweetContent,\n        user: {\n          avatar: \"https://pbs.twimg.com/profile_images/1121328878142853120/e-rpjoJi_bigger.png\",\n          name: \"User Name\",\n          handle: \"@username\"\n        },\n        created_at: new Date().toLocaleString(),\n        retweets: 0,\n        likes: 0,\n        comments: 0\n      };\n      axios.post('http://localhost:3000/tweets', newTweet).then(response => {\n        this.tweets.push(response.data); // Assuming your API returns the new tweet object with an ID\n        this.newTweetContent = ''; // Clear input field\n      }).catch(error => {\n        console.error(\"There was an error adding the tweet!\", error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","tweets","newTweetContent","mounted","fetchTweets","methods","get","then","response","catch","error","console","addTweet","newTweet","content","user","avatar","handle","created_at","Date","toLocaleString","retweets","likes","comments","post","push"],"sources":["C:\\Users\\Master\\OneDrive\\Documents\\VueJs\\projet-vuejs3-basic\\src\\components\\Main.vue"],"sourcesContent":["<template>\r\n  <div class=\"main border border-gray-600 h-auto border-t-0\">\r\n    <!-- Middle create tweet -->\r\n    <div class=\"flex\">\r\n      <div class=\"m-2 w-10 py-1\">\r\n        <img class=\"inline-block h-10 w-10 rounded-full\" src=\"https://pbs.twimg.com/profile_