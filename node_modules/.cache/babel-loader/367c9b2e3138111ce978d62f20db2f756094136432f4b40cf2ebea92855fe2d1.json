{"ast":null,"code":"import SidebarComponent from './components/Sidebar.vue';\nimport Main from './components/Main.vue';\nexport default {\n  name: 'App',\n  components: {\n    'sidebar-app': SidebarComponent,\n    'main-app': Main\n  }\n};","map":{"version":3,"names":["SidebarComponent","Main","name","components"],"sources":["C:\\Users\\Master\\OneDrive\\Documents\\VueJs\\projet-vuejs3-basic\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <div class=\"bg-black\">\n      <div class=\"container\">\n        <div class=\"side\">\n          <sidebar-app />\n        </div>\n        <main class=\"main-app\">\n          <router-view />\n        </main>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport SidebarComponent from './components/Sidebar.vue';\nimport Main from './components/Main.vue';\n\nexport default {\n  name: 'App',\n  components: {\n    'sidebar-app': SidebarComponent,\n    'main-app': Main,\n  }\n};\n</script>\n\n<style>\n.side {\n  position: fixed;\n  left: 0;\n  top: 0;\n  bottom: 0;\n  width: 250px; /* Adjust width as needed */\n  background-color: #333; /* Sidebar background color */\n}\n\n.main-app {\n  margin-left: 250px; /* Ensure main content doesn't overlap sidebar */\n  padding: 20px;\n  width: calc(100% - 250px); /* Calculate width excluding sidebar */\n}\n\n.bg-black {\n  background-color: #000; /* Background color for the entire app */\n  color: #fff; /* Text color for the entire app */\n}\n\n.container {\n  display: flex;\n  min-height: 100vh;\n}\n</style>\n"],"mappings":"AAgBA,OAAOA,gBAAe,MAAO,0BAA0B;AACvD,OAAOC,IAAG,MAAO,uBAAuB;AAExC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACV,aAAa,EAAEH,gBAAgB;IAC/B,UAAU,EAAEC;EACd;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}