{"ast":null,"code":"import \"core-js/modules/web.dom-exception.stack.js\";\nimport \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'login-app',\n  data() {\n    return {\n      email: '',\n      password: '',\n      error: ''\n    };\n  },\n  methods: {\n    async login() {\n      try {\n        const response = await axios.get('http://localhost:3004/users', {\n          params: {\n            email: this.email,\n            password: this.password\n          }\n        });\n        const user = response.data[0];\n        if (user) {\n          // Simulate token generation\n          const token = btoa(`${user.email}:${user.email}`);\n          localStorage.setItem('token', token);\n          this.$router.push('/profile');\n        } else {\n          this.error = 'Invalid username or password';\n        }\n      } catch (err) {\n        this.error = 'An error occurred';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","error","methods","login","response","get","params","user","token","btoa","localStorage","setItem","$router","push","err"],"sources":["C:\\Users\\Master\\OneDrive\\Documents\\VueJs\\Blogemaster\\src\\views\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\" bg-white min-h-screen flex justify-center items-center\">\r\n    <div class=\" form grid gap-8 h-auto w-1/2\">\r\n      <div id=\"back-div\" class=\"bg-gradient-to-r from-blue-500 to-purple-500 rounded-[26px] m-4\">\r\n        <div class=\"border-[20px] border-transparent rounded-[20px] dark:bg-gray-900 bg-white shadow-lg xl:p-10 2xl:p-10 lg:p-10 md:p-10 sm:p-2 m-2\">\r\n          <h1 class=\"pt-8 pb-6 font-bold dark:text-gray-400 text-5xl text-center cursor-default\">Log in</h1>\r\n          <form @submit.prevent=\"login\" class=\"space-y-4\">\r\n            <div>\r\n              <label for=\"email\" class=\"mb-2 dark:text-gray-400 text-lg\">Email</label>\r\n              <input\r\n               