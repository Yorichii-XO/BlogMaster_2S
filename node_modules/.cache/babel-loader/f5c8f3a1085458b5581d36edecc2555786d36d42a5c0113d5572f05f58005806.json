{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'TodoForm',\n  props: {\n    todo: Object\n  },\n  data() {\n    return {\n      todoTitle: this.todo ? this.todo.title : '',\n      todoCategory: this.todo ? this.todo.category : '',\n      categories: []\n    };\n  },\n  created() {\n    this.fetchCategories();\n  },\n  methods: {\n    fetchCategories() {\n      axios.get('http://localhost:3002/categories').then(response => {\n        this.categories = response.data;\n      }).catch(error => {\n        console.error(\"There was an error fetching the categories:\", error);\n      });\n    },\n    saveTodo() {\n      if (this.todo) {\n        axios.put(`http://localhost:3001/todos/${this.todo.id}`, {\n          title: this.todoTitle,\n          category: this.todoCategory\n        }).then(() => {\n          this.$emit('todoUpdated');\n          this.$emit('closeForm');\n        }).catch(error => {\n          console.error(\"There was an error updating the todo:\", error);\n        });\n      } else {\n        axios.post('http://localhost:3001/todos', {\n          title: this.todoTitle,\n          category: this.todoCategory\n        }).then(() => {\n          this.$emit('todoCreated');\n          this.$emit('closeForm');\n        }).catch(error => {\n          console.error(\"There was an error creating the todo:\", error);\n        });\n      }\n    }\n  },\n  watch: {\n    todo: {\n      handler(newVal) {\n        if (newVal) {\n          this.todoTitle = newVal.title;\n          this.todoCategory = newVal.category;\n        }\n      },\n      immediate: true\n    }\n  }\n};","map":{"version":3,"names":["axios","name","props","todo","Object","data","todoTitle","title","todoCategory","category","categories","created","fetchCategories","methods","get","then","response","catch","error","console","saveTodo","put","id","$emit","post","watch","handler","newVal","immediate"],"sources":["C:\\Users\\Master\\OneDrive\\Documents\\VueJs\\Blogemaster\\src\\views\\TodoForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"p-4 bg-white shadow-lg rounded-lg max-w-md mx-auto\">\r\n    <h2 class=\"text-xl font-semibold mb-4\">{{ todo ? 'Edit Todo' : 'Add Todo' }}</h2>\r\n    <form @submit.prevent=\"saveTodo\">\r\n      <div class=\"mb-4\">\r\n        <label for=\"title\" class=\"block text-gray-700 mb-2\">Title:</label>\r\n        <input\r\n          id=\"title\"\r\n          v-model=\"todoTitle\"\r\n          type=\"text\"\r\n          class=\"w-full px-3 py-2 border border-gray-300 rounded\"\r\n          required\r\n        />\r\n      </div>\r\n      <div class=\"mb-4\">\r\n        <label for=\"category\" class=\"block text-gray-700 mb-2\">Category:</label>\r\n        <select\r\n          id=\"category\"\r\n          v-model=\"todoCategory\"\r\n          class=\"w-full px-3 py-2 border border-gray-300 rounded\"\r\n          required\r\n        >\r\n          <option v-for=\"category in categories\" :key=\"category.id\" :value=\"category.id\">\r\n            {{ category.name }}\r\n          </option>\r\n        </select>\r\n      </div>\r\n      <div class=\"flex justify-end\">\r\n        <button\r\n          type=\"submit\"\r\n          class=\"px-4 py-2 bg-blue-500 text-white rounded hover:bg-blue-600\"\r\n        >\r\n          {{ todo ? 'Update' : 'Add' }}\r\n        </button>\r\n        <button\r\n          type=\"button\"\r\n          @click=\"$emit('closeForm')\"\r\n          class=\"ml-2 px-4 py-2 bg-gray-500 text-white rounded hover:bg-gray-600\"\r\n        >\r\n          Cancel\r\n        </button>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'TodoForm',\r\n  props: {\r\n    todo: Object,\r\n  },\r\n  data() {\r\n    return {\r\n      todoTitle: this.todo ? this.todo.title : '',\r\n      todoCategory: this.todo ? this.todo.category : '',\r\n      categories: [],\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchCategories();\r\n  },\r\n  methods: {\r\n    fetchCategories() {\r\n      axios.get('http://localhost:3002/categories')\r\n        .then(response => {\r\n          this.categories = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.error(\"There was an error fetching the categories:\", error);\r\n        });\r\n    },\r\n    saveTodo() {\r\n      if (this.todo) {\r\n        axios.put(`http://localhost:3001/todos/${this.todo.id}`, {\r\n          title: this.todoTitle,\r\n          category: this.todoCategory\r\n        })\r\n        .then(() => {\r\n          this.$emit('todoUpdated');\r\n          this.$emit('closeForm');\r\n        })\r\n        .catch(error => {\r\n          console.error(\"There was an error updating the todo:\", error);\r\n        });\r\n      } else {\r\n        axios.post('http://localhost:3001/todos', {\r\n          title: this.todoTitle,\r\n          category: this.todoCategory\r\n        })\r\n        .then(() => {\r\n          this.$emit('todoCreated');\r\n          this.$emit('closeForm');\r\n        })\r\n        .catch(error => {\r\n          console.error(\"There was an error creating the todo:\", error);\r\n        });\r\n      }\r\n    },\r\n  },\r\n  watch: {\r\n    todo: {\r\n      handler(newVal) {\r\n        if (newVal) {\r\n          this.todoTitle = newVal.title;\r\n          this.todoCategory = newVal.category;\r\n        }\r\n      },\r\n      immediate: true,\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Additional scoped styles if needed */\r\n</style>\r\n"],"mappings":"AA+CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACLC,IAAI,EAAEC;EACR,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI,CAACH,IAAG,GAAI,IAAI,CAACA,IAAI,CAACI,KAAI,GAAI,EAAE;MAC3CC,YAAY,EAAE,IAAI,CAACL,IAAG,GAAI,IAAI,CAACA,IAAI,CAACM,QAAO,GAAI,EAAE;MACjDC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,eAAe,CAAC,CAAC;EACxB,CAAC;EACDC,OAAO,EAAE;IACPD,eAAeA