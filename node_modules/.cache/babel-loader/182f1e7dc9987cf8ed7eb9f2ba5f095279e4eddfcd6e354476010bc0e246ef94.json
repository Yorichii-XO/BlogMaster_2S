{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'TodoList',\n  data() {\n    return {\n      todos: []\n    };\n  },\n  created() {\n    this.fetchTodos();\n  },\n  methods: {\n    fetchTodos() {\n      axios.get('http://localhost:3001/todos').then(response => {\n        this.todos = response.data;\n      }).catch(error => {\n        console.error(\"There was an error fetching the todos:\", error);\n      });\n    },\n    updateStatus(todo) {\n      axios.put(`http://localhost:3001/todos/${todo.id}`, todo).then(() => {\n        console.log(\"Todo status updated\");\n      }).catch(error => {\n        console.error(\"There was an error updating the todo status:\", error);\n      });\n    },\n    deleteTodo(id) {\n      axios.delete(`http://localhost:3001/todos/${id}`).then(() => {\n        this.fetchTodos();\n      }).catch(error => {\n        console.error(\"There was an error deleting the todo:\", error);\n      });\n    },\n    goToTodoDetail(id) {\n      this.$router.push({\n        name: 'EditTodo',\n        params: {\n          id\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","todos","created","fetchTodos","methods","get","then","response","catch","error","console","updateStatus","todo","put","id","log","deleteTodo","delete","goToTodoDetail","$router","push","params"],"sources":["C:\\Users\\Master\\OneDrive\\Documents\\VueJs\\Blogemaster\\src\\views\\TodoList.vue"],"sourcesContent":["<template>\r\n  <div class=\"p-6 bg-gray-100 min-h-screen\">\r\n    <h2 class=\"text-2xl font-bold mb-4\">Todo List</h2>\r\n\r\n    <!-- Todos List in Flexbox -->\r\n    <div class=\"flex flex-wrap gap-4\">\r\n      <div v-for=\"todo in todos\" :key=\"todo.id\" class=\"flex flex-col p-4 bg-black text-white rounded-lg shadow-md cursor-pointer transition-transform transform hover:scale-105\" @click=\"goToTodoDetail(todo.id)\">\r\n        <h3 class=\"text-xl font-semibold mb-2\">{{ todo.title }}</h3>\r\n        <p class=\"text-gray-400 mb-2\">Due Date: {{ todo.dueDate }}</p>\r\n        <div class=\"mb-2\">\r\n          <label for=\"status\" class=\"block text-sm font-medium\">Status:</label>\r\n          <select v-model=\"todo.status\" @change=\"updateStatus(todo)\" id=\"status\" class=\"p-2 border rounded w-full bg-gray-700 text-white\">\r\n            <option value=\"To Do\">To Do</option>\r\n            <option value=\"In Progress\">In Progress</option>\r\n            <option value=\"Done\">Done</option>\r\n          </select>\r\n        </div>\r\n        <div class=\"mt-auto flex space-x-2\">\r\n          <router-link :to=\"{ name: 'EditTodo', params: { id: todo.id } }\" class=\"px-2 py-1 bg-blue-500 text-white rounded hover:bg-blue-600\">Edit</router-link>\r\n          <button @click.stop=\"deleteTodo(todo.id)\" class=\"px-2 py-1 bg-red-500 text-white rounded hover:bg-red-600\">Delete</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Add Todo Button -->\r\n    <router-link to=\"/add-todo\" class=\"mt-4 px-4 py-2 bg-green-500 text-white rounded hover:bg-green-600 inline-block\">Add Todo</router-link>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'TodoList',\r\n  data() {\r\n    return {\r\n      todos: []\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchTodos();\r\n  },\r\n  methods: {\r\n    fetchTodos() {\r\n      axios.get('http://localhost:3001/todos')\r\n        .then(response => {\r\n          this.todos = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.error(\"There was an error fetching the todos:\", error);\r\n        });\r\n    },\r\n    updateStatus(todo) {\r\n      axios.put(`http://localhost:3001/todos/${todo.id}`, todo)\r\n        .then(() => {\r\n          console.log(\"Todo status updated\");\r\n        })\r\n        .catch(error => {\r\n          console.error(\"There was an error updating the todo status:\", error);\r\n        });\r\n    },\r\n    deleteTodo(id) {\r\n      axios.delete(`http://localhost:3001/todos/${id}`)\r\n        .then(() => {\r\n          this.fetchTodos();\r\n        })\r\n        .catch(error => {\r\n          console.error(\"There was an error deleting the todo:\", error);\r\n        });\r\n    },\r\n    goToTodoDetail(id) {\r\n      this.$router.push({ name: 'EditTodo', params: { id } });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Additional scoped styles if needed */\r\n</style>\r\n"],"mappings":";AA8BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACPD,UAAUA,CAAA,EAAG;MACXL,KAAK,CAACO,GAAG,CAAC,6BAA6B,EACpCC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACN,KAAI,GAAIM,QAAQ,CAACP,IAAI;MAC5B,CAAC,EACAQ,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,wCAAwC,EAAEA,KAAK,CAAC;MAChE,CAAC,CAAC;IACN,CAAC;IACDE,YAAYA,CAACC,IAAI,EAAE;MACjBd,KAAK,CAACe,GAAG,CAAE,+BAA8BD,IAAI,CAACE,EAAG,EAAC,EAAEF,IAAI,EACrDN,IAAI,CAAC,MAAM;QACVI,OAAO,CAACK,GAAG,CAAC,qBAAqB,CAAC;MACpC,CAAC,EACAP,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,8CAA8C,EAAEA,KAAK,CAAC;MACtE,CAAC,CAAC;IACN,CAAC;IACDO,UAAUA,CAACF,EAAE,EAAE;MACbhB,KAAK,CAACmB,MAAM,CAAE,+BAA8BH,EAAG,EAAC,EAC7CR,IAAI,CAAC,MAAM;QACV,IAAI,CAACH,UAAU,CAAC,CAAC;MACnB,CAAC,EACAK,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,uCAAuC,EAAEA,KAAK,CAAC;MAC/D,CAAC,CAAC;IACN,CAAC;IACDS,cAAcA,CAACJ,EAAE,EAAE;MACjB,IAAI,CAACK,OAAO,CAACC,IAAI,CAAC;QAAErB,IAAI,EAAE,UAAU;QAAEsB,MAAM,EAAE;UAAEP;QAAG;MAAE,CAAC,CAAC;IACzD;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}