{"ast":null,"code":"import \"core-js/modules/web.dom-exception.stack.js\";\nimport axios from 'axios';\nexport default {\n  name: 'PostComponent',\n  data() {\n    return {\n      posts: [],\n      isAuthenticated: false,\n      userId: null // Add this line to track the authenticated user's ID\n    };\n  },\n\n  mounted() {\n    this.checkAuthentication();\n    this.fetchPosts();\n  },\n  methods: {\n    checkAuthentication() {\n      // Check if the user is authenticated\n      const token = localStorage.getItem('token');\n      this.isAuthenticated = !!token; // Update isAuthenticated based on token presence\n      if (this.isAuthenticated) {\n        // Decode the token to get the user ID (assuming the token contains the user ID)\n        const decodedToken = JSON.parse(atob(token.split('.')[1]));\n        this.userId = decodedToken.userId;\n      }\n    },\n    async fetchPosts() {\n      try {\n        const response = await axios.get('http://localhost:3000/posts');\n        this.posts = response.data;\n      } catch (error) {\n        console.error('Error fetching posts:', error);\n      }\n    },\n    async deletePost(postId) {\n      if (this.isAuthenticated) {\n        try {\n          await axios.delete(`http://localhost:3000/posts/${postId}`);\n          this.posts = this.posts.filter(post => post.id !== postId);\n        } catch (error) {\n          console.error('Error deleting post:', error);\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","posts","isAuthenticated","userId","mounted","checkAuthentication","fetchPosts","methods","token","localStorage","getItem","decodedToken","JSON","parse","atob","split","response","get","error","console","deletePost","postId","delete","filter","post","id"],"sources":["C:\\Users\\Master\\OneDrive\\Documents\\VueJs\\Blogemaster\\src\\components\\Main.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <div class=\"relative\">\r\n        <!-- Gradient Overlay -->\r\n        <