ript>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'MainContent',\r\n  data() {\r\n    return {\r\n      tweetContent: '',\r\n      tweets: [\r\n        {\r\n          id: 1,\r\n          author: 'Sonal Hirave',\r\n          username: 'sonalihirave',\r\n          time: '1h',\r\n          content: \"I've had a look at the new features in Tailwind CSS v1.7.0 – it's awesome! 😍 #TailwindCSS #WebDev\"\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  methods: {\r\n    addTweet() {\r\n      this.postTweet(this.tweetContent);\r\n      this.tweetContent = ''; // Clear the textarea after posting\r\n    },\r\n    postTweet(content) {\r\n      axios.post('/api/tweets', { content })\r\n        .then(response => {\r\n          console.log('Tweet posted successfully:', response.data);\r\n          // Add the new tweet to the beginning of the tweets array\r\n          this.tweets.unshift({\r\n            id: response.data.id,\r\n            author: response.data.author,\r\n            username: response.data.username,\r\n            time: response.data.time,\r\n            content: response.data.content\r\n          });\r\n        })\r\n        .catch(error => {\r\n          console.error('Error posting tweet:', error);\r\n          // Handle error scenarios\r\n        });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.main {\r\n  background-color: #192734;\r\n  border-radius: 20px 20px 0 0;\r\n}\r\n</style>\r\n"],"mappings":";AAuGA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,EAAE;MAChBC,MAAM,EAAE,CACN;QACEC,EAAE,EAAE,CAAC;QACLC,MAAM,EAAE,cAAc;QACtBC,QAAQ,EAAE,cAAc;QACxBC,IAAI,EAAE,IAAI;QACVC,OAAO,EAAE;MACX;IAEJ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACT,IAAI,CAACC,SAAS,CAAC,IAAI,CAACT,YAAY,CAAC;MACjC,IAAI,CAACA,YAAW,GAAI,EAAE,EAAE;IAC1B,CAAC;;IACDS,SAASA,CAACH,OAAO,EAAE;MACjBT,KAAK,CAACa,IAAI,CAAC,aAAa,EAAE;QAAEJ;MAAQ,CAAC,EAClCK,IAAI,CAACC,QAAO,IAAK;QAChBC,OAAO,CAACC,GAAG,CAAC,